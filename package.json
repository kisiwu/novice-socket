{
  "name": "@novice1/socket",
  "private": false,
  "version": "0.4.5",
  "description": "Simplifies creating server-side Socket.IO applications by providing namespace and event builders, middleware support, and utilities for robust real-time communication.",
  "main": "lib/index.js",
  "homepage": "https://kisiwu.github.io/novice-socket/latest/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kisiwu/novice-socket.git"
  },
  "scripts": {
    "doc": "typedoc",
    "build": "tsc",
    "lint": "eslint .",
    "test": "kaukau --require ts-node/register --config kaukau-config-src.json",
    "testlib": "kaukau --config kaukau-config-lib.json",
    "format": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --write ./src",
    "format:check": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --check ./src",
    "format:staged": "cross-env NODE_OPTIONS=--experimental-strip-types lint-staged"
  },
  "author": "demingongo",
  "license": "MIT",
  "dependencies": {
    "array-flatten": "^3.0.0",
    "socket.io": "^4.8.1",
    "socket.io-adapter": "^2.5.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "chai": "^4.5.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "express": "^5.2.1",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "kaukau": "^4.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}